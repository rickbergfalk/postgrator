{
  "name": "postgrator",
  "version": "4.2.0",
  "author": "Rick Bergfalk <rick.bergfalk@gmail.com>",
  "description": "A SQL migration tool for SQL people",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "migrator",
    "migration",
    "postgres",
    "postgresql",
    "mysql",
    "sql server",
    "sql"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint \"**/*.js\"",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.{js,ts}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "depd": "^2.0.0",
    "glob": "^7.2.0",
    "newline": "0.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rickbergfalk/postgrator.git"
  },
  "main": "postgrator",
  "types": "postgrator.d.ts",
  "devDependencies": {
    "@types/mocha": "^8.2.3",
    "@types/node": "^13.13.30",
    "@types/pg": "^8.6.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.0",
    "lint-staged": "^10.5.0",
    "mocha": "^8.4.0",
    "mssql": "^7.2.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.0",
    "pg": "^8.7.1",
    "prettier": "^2.4.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.3"
  }
}
